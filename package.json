{
  "name": "openapi-ts-define",
  "version": "0.4.0",
  "description": "This package provide a way to extract api route information based on AST.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">= 18"
  },
  "scripts": {
    "dev": "npm run test:watch",
    "test": "vitest run",
