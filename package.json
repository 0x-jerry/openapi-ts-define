  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@0x-jerry/x-release": "^2.2.2",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.14.11",
    "@vitest/coverage-v8": "^2.0.3",
    "openapi-typescript": "^7.0.4",
    "tsup": "^8.1.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vitest": "^2.0.3"
  },
  "dependencies": {
    "@0x-jerry/utils": "^3.0.0",
    "fast-glob": "^3.3.2",
    "ts-morph": "^24.0.0"
