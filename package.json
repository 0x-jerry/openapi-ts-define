{
  "name": "openapi-ts-define",
  "version": "0.7.0",
  "description": "This package provide a way to extract api route information based on AST.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">= 18"
  },
  "scripts": {
    "dev": "tsx src/dev.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "prepublishOnly": "npm run build",
    "release": "x-release"
  },
  "license": "MIT",
  "author": "0x-jerry <x.jerry.wang@gmail.com>",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@0x-jerry/x-release": "^2.2.2",
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^3.0.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@0x-jerry/utils": "^4.2.1",
    "@types/json-schema": "^7.0.15",
    "fast-glob": "^3.3.3",
    "openapi-typescript": "^7.5.2",
    "ts-morph": "^25.0.0"
  },
  "stackblitz": {
    "startCommand": "npm run test:watch"
  }
}